version 12
hostname R7
!
no ip domain lookup
!
ipv6 unicast-routing
!
interface Loopback0
 ip address 7.7.7.7 255.255.255.255
 ipv6 address 2001:7:7:7::7/128
 no shutdown
interface GigabitEthernet1.57
 encapsulation dot1Q 57
 ip address 10.1.57.7 255.255.255.0
 ipv6 address 2001:10:1:57::7/64
 no shutdown
!
end
